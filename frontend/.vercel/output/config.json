{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/api\\/auth(?:\\/(.*?))?$",
			"dest": "_render"
		},
		{
			"src": "^\\/create-exercise\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/demos\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/login\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/search-exercise\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/see-my-exercise\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/sign-up\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/$",
			"dest": "_render"
		}
	]
}